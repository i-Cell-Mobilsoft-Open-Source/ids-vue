{
  "name": "@i-cell/ids-vue",
  "version": "1.0.38",
  "description": "i-ds",
  "private": false,
  "type": "module",
  "files": [
    "dist",
    "*.d.ts",
    "*.css",
    "*.js",
    "./src/style.css",
    "./src/styles/ids-tokens.css"
  ],
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "umd": "dist/index.umd.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    },
    "./dist/*.css": {
      "import": "./dist/*.css",
      "require": "./dist/*.css"
    }
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/i-Cell-Mobilsoft-Open-Source/ids-vue.git"
  },
  "keywords": [
    "i-ds",
    "vue",
    "typescript"
  ],
  "scripts": {
    "dev": "vite",
    "preview": "vite preview",
    "prepare": "husky install",
    "format": "prettier . --write",
    "build": "vue-tsc && vite build && npm run build:css",
    "build:css": "sass src/styles/index.scss dist/index.css --no-source-map --style compressed",
    "lint": "eslint . --fix --max-warnings=0",
    "generate:css": "node scripts/token-parser.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "eslint 'src/**' --fix",
      "npm run test:staged"
    ]
  },
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^26.0.1",
    "@types/cypress": "^1.1.3",
    "@types/node": "^20.14.8",
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vue/eslint-config-typescript": "^13.0.0",
    "autoprefixer": "^10.4.19",
    "cypress": "^13.12.0",
    "cypress-real-events": "^1.13.0",
    "eslint": "^8.57.0",
    "eslint-plugin-cypress": "^3.3.0",
    "eslint-plugin-no-only-tests": "^3.1.0",
    "eslint-plugin-vue": "^9.26.0",
    "husky": "^9.0.11",
    "postcss": "^8.4.38",
    "postcss-nesting": "^12.1.5",
    "prettier": "3.3.2",
    "sass": "^1.77.6",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.5.2",
    "vite": "^5.3.1",
    "vue-tsc": "^2.0.22"
  },
  "dependencies": {
    "@heroicons/vue": "^2.1.4",
    "@i-cell/ids-tokens": "^0.0.8",
    "@rushstack/eslint-patch": "^1.10.3",
    "@vue-hero-icons/outline": "^1.7.2",
    "vite-plugin-dts": "^3.9.1",
    "vue-router": "^4.4.0",
    "vue-transition": "^0.1.3"
  }
}
